# Copy this file to .env and fill in your actual values
# NEVER commit .env to git

# Local database (source)
LOCAL_DB_HOST=localhost
LOCAL_DB_PORT=5433
LOCAL_DB_NAME=gisdb
LOCAL_DB_USER=postgres
LOCAL_DB_PASSWORD=your_password_here

# Supabase (target) â€” use Session Pooler for IPv4 compatibility
# Find these at: Supabase Dashboard > Connect > Connection String > Session Pooler
SUPABASE_DB_HOST=aws-1-us-east-1.pooler.supabase.com
SUPABASE_DB_PORT=5432
SUPABASE_DB_NAME=postgres
SUPABASE_DB_USER=postgres.your-project-ref
SUPABASE_DB_PASSWORD=your_supabase_db_password

# Supabase API (for REST/GeoJSON access)
SUPABASE_URL=https://your-project-ref.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
